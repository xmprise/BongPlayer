<?xml version="1.0" encoding="utf-8"?>
<layer-list
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:id="@android:id/background">
        <selector>
            <item android:state_focused="true">
                <shape>
                    <stroke android:width="1.0px" android:color="#aaaaaaaa" />
                    <solid android:color="#20ffffff" />
                </shape>
            </item>
            <item>
                <shape>
                    <stroke android:width="1.0px" android:color="#99999999" />
                    <solid android:color="#00000000" />
                </shape>
            </item>
        </selector>
    </item>
    <item android:id="@android:id/secondaryProgress">
        <clip>
            <selector>
                <item android:state_focused="true">
                    <shape>
                        <stroke android:width="4.0dip" android:color="#00000000" />
                        <solid android:color="#a0ffffff" />
                    </shape>
                </item>
                <item>
                    <shape>
                        <stroke android:width="4.0dip" android:color="#00000000" />
                        <solid android:color="#50ffffff" />
                    </shape>
                </item>
            </selector>
        </clip>
    </item>
    <item android:id="@android:id/progress">
        <clip>
            <selector>
                <item android:state_pressed="true">
                    <shape>
                        <stroke android:width="4.0dip" android:color="#00000000" />
                        <solid android:color="@color/video_progress_press" />
                    </shape>
                </item>
                <item android:state_focused="true">
                    <shape>
                        <stroke android:width="4.0dip" android:color="#00000000" />
                        <solid android:color="@color/video_progress_press" />
                    </shape>
                </item>
                <item>
                    <shape>
                        <stroke android:width="4.0dip" android:color="#00000000" />
                        <solid android:color="@color/video_progress_normal" />
                    </shape>
                </item>
            </selector>
        </clip>
    </item>
</layer-list>
